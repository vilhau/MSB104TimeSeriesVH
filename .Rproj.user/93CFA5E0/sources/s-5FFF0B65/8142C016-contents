hello <- function() {
  print("Hello, world!")
}
hello()

hello <- function(name=NULL){
  tekst <- paste("Hello world! My name is", name)
  print(tekst)
}

hello("Vilde")

#`This is a hello function with a name argument
#Â´@export
hello <- function(){
  print("Hello, world! My name is Vilde")
}

